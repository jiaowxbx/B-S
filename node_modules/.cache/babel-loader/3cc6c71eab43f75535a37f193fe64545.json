tem:nth-child(2n+1) {\r\n    background-color: #d3dce6;\r\n  }\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nconst ax=axios.create({\r\n    baseURL:'http://43.138.41.164:8080/getInfo',\r\n\r\n})\r\nexport default {\r\n  \r\n  name:'chaxun',\r\n  data(){\r\n    return {\r\n       options: [\r\n\r\n       ],\r\n      cropsVos:[\r\n\r\n      ],\r\n        value: '',\r\n        Vvalue:[],\r\n        solve:'',\r\n        \r\n    }\r\n  },\r\n  mounted(){\r\n    ax({\r\n      method:'GET',\r\n      url:`http://43.138.41.164:8080/system/detail/list`+`?pageNum=2&pageSize=10`,\r\n      headers:{Authorization:localStorage.getItem('Authorization')}\r\n    }).then(res=>{\r\n      \r\n      console.log('*.*');\r\n      console.log(res.data.rows);\r\n      console.log(this);\r\n      console.log('123');\r\n     \r\n  \r\n    })\r\n  },\r\n  methods:{\r\n    changeSelect() {\r\n          ax({\r\n              method:'GET',\r\n              url:`${this.value}`\r\n          }).then(res=>{\r\n            console.log('000');\r\n            for(var i=0;i<res.data.data.cropsVos.length;i++){\r\n              if(this.Vvalue==res.data.data.cropsVos[i].detail){\r\n                this.solve=res.data.data.cropsVos[i].solve\r\n              }\r\n            }\r\n        }).catch(err=>{\r\n            console.log(err);\r\n        })\r\n      },\r\n      getCname(){\r\n          ax({\r\n              method:'GET',\r\n              url:`${this.value}`\r\n          }).then(res=>{\r\n            console.log(this);\r\n            this.cropsVos=res.data.data.cropsVos\r\n            \r\n            console.log(res);\r\n            \r\n        }).catch(err=>{\r\n            console.log(err);\r\n        })\r\n      },\r\n      \r\n    // getCname(){\r\n    //     axios\r\n        \r\n    //         .get(\"http://119.3.182.104:8080/crop/detail/find\",{params:{cname:this.value}})\r\n    //         // url:`?username=${this.name}&password=${this.password}`,\r\n    //         .then((data)=>{\r\n    //             console.log('...');\r\n    //             console.log(data);\r\n    //         })\r\n    // }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n#jieguo{\r\n    margin: 20px 0 0 0;\r\n    /* background-color: rgb(84, 92, 100); */\r\n}\r\n#chaxun{\r\n  margin-top: 20px;\r\n}\r\n</style>"],"sourceRoot":"src/login"}]}